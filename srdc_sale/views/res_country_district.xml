<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
     <record model="ir.ui.view" id="res_country_district_tree">
        <field name="name">res.country.district.tree</field>
        <field name="model">res.country.district</field>
        <field name="arch" type="xml">
            <tree string="Country District" editable="bottom">
                <field name="name"/>
                <field name="state_id"/>
            </tree>
        </field>
    </record>

     <record model="ir.actions.act_window" id="res_country_district_list_action">
        <field name="name">Country Districts</field>
        <field name="res_model">res.country.district</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>