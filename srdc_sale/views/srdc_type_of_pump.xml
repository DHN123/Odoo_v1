<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--Tree View-->
    <record id="srdc_type_of_pump_tree" model="ir.ui.view">
        <field name="name">SRDC Type of Pump Tree</field>
        <field name="model">srdc.type.of.pump</field>
        <field name="arch" type="xml">
            <tree string="SRDC Type of Pump" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <!--Search View-->
    <record id="srdc_type_of_pump_search" model="ir.ui.view">
        <field name="name">SRDC Type of Pump Search</field>
        <field name="model">srdc.type.of.pump</field>
        <field name="arch" type="xml">
            <search string="SRDC Type of Pump">
                <field name="name"/>
            </search>
        </field>
    </record>

    <record id="srdc_type_of_pump_action" model="ir.actions.act_window">
        <field name="name">SRDC Type of Pump</field>
        <field name="res_model">srdc.type.of.pump</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>